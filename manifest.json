{
    "manifest_version": 2,
    "name": "Find work extension",
    "version": "0.1.0",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "permissions": [
        "activeTab",
        "<all_urls>",
        "tabs"
    ],
    "browser_action": {
        "default_popup": "popup/popup.html",
        "default_title": "A popup will come here",
        "default_icon": "assets/icon-32x32.png"
    }
}